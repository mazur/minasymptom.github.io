{
  "name": "@cocheck/workspace",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "eslint": "6",
    "eslint-config-prettier": "6",
    "eslint-config-react": "1",
    "eslint-config-react-app": "5",
    "eslint-plugin-babel": "5",
    "eslint-plugin-flowtype": "4",
    "eslint-plugin-import": "2",
    "eslint-plugin-jsx-a11y": "6",
    "eslint-plugin-prettier": "3",
    "eslint-plugin-promise": "4",
    "eslint-plugin-react": "7",
    "eslint-plugin-react-hooks": "3",
    "husky": "4",
    "prettier": "2",
    "pretty-quick": "2",
    "react": "16",
    "react-scripts": "3",
    "typescript": "3",
    "react-test-renderer": "16"
  },
  "scripts": {
